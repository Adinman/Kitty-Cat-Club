<body>
    <center>
        <div>
            <div class="displayiframe">
                <iframe src="" id="result" height="50vh"></iframe>
            </div>
            <div class="main" onmouseover=" detecthiddenui()" onmouseleave="inputlostfocus = 0; hideui()">
                <div class="align-left">
                    <button class="navbtn" onclick="document.getElementById('result').contentWindow.history.go(-1);">
                        <i class="fa-solid fa-angle-left"></i>
                    </button>
                    <button class="navbtn"
                        onclick="document.getElementById('result').contentWindow.location.reload(true);">
                        <i class="fa-solid fa-rotate-right"></i>
                    </button>
                    <button class="navbtn" onclick="document.getElementById('result').contentWindow.history.go(1);">
                        <i class="fa-solid fa-angle-right"></i>
                    </button>
                    <button class="navbtn" onclick="popoutWindow()">
                        <i class="fa-solid fa-up-right-from-square"></i>
                    </button>
                    <button class="navbtn" onclick="devTools()">
                        <i class="fa-solid fa-code"></i>
                    </button>
                </div>

                <div style="display: flex; justify-content: center;
                margin-left: 20px; margin-right: 20px;">
                    <form id="searchform">
                        <input class="searchinput" placeholder="Search the web or enter a URL. . ."
                            onfocus="keepuiopen = 1; dontchangeurl = 1"
                            onblur="keepuiopen = 0; dontchangeurl = 0; inputlostfocus = 1; hideui()">
                    </form>
                </div>

                <div class="align-right">
                    <button class="navbtn" onclick="location.replace('/dashboard')">
                        <i class="fa-solid fa-home"></i>
                    </button>
                    <button class="navbtn" onclick="toggleui()">
                        <i class="fa-solid fa-eye-slash"></i>
                    </button>
                    <button class="navbtn" onclick="aboutBlank()">
                        <i class="fa-solid fa-masks-theater"></i>
                    </button>
                    <button class="navbtn" onclick="fullscreen()">
                        <i class="fa-solid fa-expand"></i>
                    </button>
                    <button class="navbtn" onclick="togglesettings()">
                        <i class="fa-solid fa-gear"></i>
                    </button>
                </div>
            </div>
    </center>
    <button class="toggleuibtn" onclick="toggleui()">
        <p style="margin-top: -10px;"><i class="fa-solid fa-x"></i></p>
    </button>
    <div class="menu">
        <h1 style="margin-top: -1px;">Settings Panel</h1>
        <p style="margin-top: -7px; text-align: start;">A Settings Menu that can be used for your customization.</p>
        <center class="container">
            <div>
                <!--<button onclick="toggleClock()">Toggle Clock</button>-->
                <button id="navbarbtn" onclick="
                if (localStorage.getItem('autohideui') == 0) {
                    localStorage.setItem('autohideui', 1);
                    alert('Reloading the page for changes to take effect..')
                location.reload();
                } else if (localStorage.getItem('autohideui') == 1) {
                    localStorage.setItem('autohideui', 0);
                    alert('Reloading the page for changes to take effect..');
                location.reload();
                }
                ">Auto-hide Navbar</button>

                <button id="copy" onclick="    
                let temp = document.createElement('input');
                temp.value = document.getElementById('result').contentWindow.location.href;
                document.body.appendChild(temp);
                temp.select();
                temp.setSelectionRange(0, 99999);
                document.execCommand('copy');
                document.body.removeChild(temp);
                getElementById('copy').innerHTML = 'Copied!';
                delay(2000).then(() => {
                    getElementById('copy').innerHTML = 'Copy Encoded URL';
                });
                doc">Copy Encoded URL</button>

                <button onclick="
                document.querySelector('.main').style.display = 'none';
                document.querySelector('.menu').style.display = 'none';
                document.querySelector('.clockdisplay').style.display = 'none';
                document.getElementById('result').style.top = 0;
                document.getElementById('result').style.height = 'calc(100vh)';
                ">Destroy UI</button>
            </div>
            <h2>== Other stuff ==</h2>
            <button onclick="window.open('https://discord.com/invite/rqBgxP9Gnj').focus();">Join Discord Server</button>
            <button
                onclick="window.open('https://docs.google.com/forms/d/e/1FAIpQLSdwOhdF3YtIx6IVQE5UA81bmQ_4a8QkIOu-WaC8mAyG9YHT6A/viewform?usp=sf_link').focus();">Report
                a bug</button>
        </center>
        <br />
    </div>
    <center>
        <div class="clockdisplay">
            <p id="clock">12:00:00 A.M.</p>
        </div>
    </center>
    </div>
</body>
<script>
    const isLoggedIn = localStorage.getItem("loggedin");
    if (!isLoggedIn) {
      location.href = "/";
    }
  </script>
<script src="./js/learninginit.js"></script>
<script src="./js/clock.js"></script>
<script src="./uv/uv.bundle.js"></script>
<script src="./uv/uv.config.js"></script>
<script src="./js/learning.js"></script>
<script>
    let savedTitle = localStorage.getItem("tabName");
    let savedBG = localStorage.getItem("BG");
    let savedFavicon = localStorage.getItem("favicon");
    let favicon = document.getElementById("icon");
    let savedLink = localStorage.getItem("savedLink");
    let savedKey = localStorage.getItem("savedKey");
    let savedIframeBackground = localStorage.getItem("loader");

    if (savedTitle) {
        document.title = savedTitle;
    }
    if (savedFavicon) {
        if (savedFavicon == './img/logo.png') {
            favicon.href = './img/math.png';
        } else {
            favicon.href = savedFavicon;
        }
    }

    if (savedBG) {
        document.body.style.backgroundImage = `url("${savedBG}")`;
        document.body.style.backgroundRepeat = "no-repeat";
        document.body.style.backgroundSize = "cover";
    }

    if (savedIframeBackground) {
        iframe.style.backgroundImage = `url('${savedIframeBackground}')`;
        iframe.style.backgroundSize = "cover";
        iframe.style.backgroundRepeat = "no-repeat";
    }

    let tabclosingval = localStorage.getItem("tabcloseval");

    window.onbeforeunload = () => {
        if (tabclosingval == "true") {
            return "Do you want to leave the current tab?";
        }
    }

    function panic(event) {
        if (event.key === savedKey) {
            focusIframe();
            if (savedLink.includes("https://")) {
                location.replace(savedLink);
            } else {
                location.replace("https://" + savedLink);
            }
        }
    }

    function focusIframe() {
        document.getElementById("result").focus();
    }

    document.addEventListener("keydown", panic);
</script>

</html>